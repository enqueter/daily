services:
  app:
    image: ghcr.io/enqueter/daily:develop
    ports:
      - 127.0.0.1:3000:3000
    working_dir: /app
    volumes:
      - warehouse:/app/warehouse
      - ~/.aws:/root/.aws:readonly
    environment:
      - AWS_CONFIG_FILE=/root/.aws/config
      - AWS_SSO_SESSION={sso_session}
volumes:
  warehouse: